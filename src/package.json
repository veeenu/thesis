{
  "name": "Thesis",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "__": "nodemon --delay 0 -e js,glsl -x npm run build",
    "start": "watchify -dv -t aliasify -t brfs -e main.js -o ../_site/build.js",
    "build": "browserify -t aliasify -t brfs -e main.js | uglifyjs -mc --screw-ie8 -o ../build.js",
    "disc": "browserify --full-paths -t aliasify -t brfs $PWD/main.js | uglifyjs --screw-ie8 -o ../build.js && discify ../build.js > ../report.html",
    "deploy": "npm run build && cd .. && git add --all . && git commit -am . && git push -u origin --all",
    "warnings": "for i in `find . -name '*.js' -and -not -ipath '*/node_modules/*'`; do echo ${i}': '; cat $i | uglifyjs -mc -o /dev/null; done",
    "env": "env"
  },
  "dependencies": {
    "brfs": "^1.4.0",
    "browserify": "^10.2.1",
    "cardinal-spline": "stbaer/cardinal-spline",
    "disc": "^1.3.2",
    "earcut": "^1.4.2",
    "gl-matrix": "^2.2.1",
    "mersennetwister": "^0.2.1",
    "stats-js": "^1.0.0-alpha1"
  },
  "devDependencies": {
    "aliasify": "^1.5.1",
    "glslify": "^2.1.2",
    "nodemon": "^1.3.7",
    "watchify": "^3.1.1"
  },
  "aliasify": {
    "aliases": {
      "glMatrix": "./lib/glMatrixSubset",
      "QuadTree": "./lib/QuadTree",
      "Loader": "./lib/Loader",
      "Timeline": "./lib/Timeline",
      "Mesh": "./lib/Mesh",
      "Context": "./Context",
      "PRNG": "./generators/PRNG",
      "Geom": "./lib/Geom",
      "Spline": "./lib/Spline",
      "ShapeGrammar": "./lib/ShapeGrammar"
    }
  },
  "author": "Andrea Venuta",
  "license": "MIT"
}

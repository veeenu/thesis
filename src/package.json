{
  "name": "Thesis",
  "version": "0.0.1",
  "description": "",
  "main": "main.js",
  "scripts": {
    "__": "nodemon --delay 0 -e js,glsl -x npm run build",
    "start": "watchify -dv -t aliasify -t brfs -e main.js -o ../build.js",
    "build": "browserify -t aliasify -t brfs -e main.js -o ../build.js",
    "disc": "browserify --full-paths -t aliasify -t brfs -e main.js -o ../build.js && discify ../build.js > ../report.html"
  },
  "dependencies": {
    "brfs": "^1.4.0",
    "earcut": "^1.4.2",
    "gl-matrix": "^2.2.1",
    "stats-js": "^1.0.0-alpha1"
  },
  "devDependencies": {
    "aliasify": "^1.5.1",
    "glslify": "^2.1.2",
    "nodemon": "^1.3.7",
    "watchify": "^3.1.1"
  },
  "aliasify": {
    "aliases": {
      "Context": "./Context",
      "PRNG": "./generators/PRNG",
      "Geom": "./lib/Geom",
      "ShapeGrammar": "./lib/ShapeGrammar"
    }
  },
  "author": "Andrea Venuta",
  "license": "MIT"
}
